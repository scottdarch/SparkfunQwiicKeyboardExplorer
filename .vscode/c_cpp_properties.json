{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${env:HOME}/Documents/Arduino/libraries/**",
                "/Applications/Arduino.app/Contents/Java/libraries/**",
                "${env:HOME}/Library/Arduino15/packages/SparkFun/hardware/avr/1.1.13/**",
                "${workspaceFolder}/src/**"
            ],
            "forcedInclude": [],
            "defines": [
                "USBCON"
            ],
            "macFrameworkPath": [
                "/System/Library/Frameworks",
                "/Library/Frameworks"
            ],
            "compilerPath": "/Applications/Arduino.app/Contents/Java/hardware/tools/avr/bin/avr-gcc",
            "compilerArgs": [
                "-c",
                "-g",
                "-Os",
                "-Wall",
                "-Wextra",
                "-std=g++11",
                "-fpermissive",
                "-fno-exceptions",
                "-ffunction-sections",
                "-fdata-sections",
                "-fno-threadsafe-statics",
                "-Wno-error=narrowing",
                "-MMD",
                "-flto"
            ]
        }
    ],
    "version": 4
}